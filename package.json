{
  "name": "testcafe_testing",
  "version": "1.0.0",
  "description": "Automation with testcafe",
  "main": "index.js",
  "dependencies": {
    "prettier": "^2.3.2",
    "testcafe": "^1.15.0"
  },
  "devDependencies": {
    "xpath-to-css": "^1.0.5"
  },
  "scripts": {
    "test:chrome": "testcafe chrome ./Tests -s takeOnFails=true",
    "test:chrome:2": "testcafe -c 2 chrome ./Tests -s takeOnFails=true",
    "test:chrome:headless:2": "testcafe -c 2 chrome ./Tests -s takeOnFails=true",
    "test:chrome:headless": "testcafe chrome:headless ./Tests",
    "test:chrome:mobile": "testcafe chrome:emulation:device=iphone x  Tests/firsttest.js"
  },
  "keywords": [
    "TestCafe",
    "Testing",
    "Automation"
  ],
  "author": "",
  "license": "ISC"
}
